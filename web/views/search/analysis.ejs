<html>
    <head>
        <title>EJS TEST</title>
        <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">

        <style>
            body{
                background: white;
                margin-top: 30px;
                margin-bottom: 30px;
                height: 100%;
            }
            .top{
                width: 100%;
                height: 30px;
                position: fixed;
                top: 0;
                background-color: dodgerblue;
            }
            .img_box{
                background-color: #cee3fe;
                width: 100%;
                height: 90%;
                margin-top: 2%;
            }
            .box1{
                margin: 100px auto;
                width: 43%;
                height: 80%;
                position: relative;
                top: 10%;
                margin-top: 2%;
                display: inline-block;
                vertical-align: middle;
                text-align: left;
                font-size: 1.5em;
            }
            .box2{
                margin: 100px auto;
                width: 43%;
                height: 80%;
                position: relative;
                top: 10%;
                margin-top: 2%;
                display: inline-block;
                vertical-align: middle;
                text-align: left;
                font-size: 1.5em;
            }
            .vertical_bar{
                width: 6px;
                height: 75%;
                background-color: dodgerblue;
                position: relative;
                display: inline-block;
                margin-top: 8%;
                margin-left: 2%;
                margin-right: 2%;
                vertical-align: middle;
            }
            .result_tag{
                position: relative;
                font-size: 2.5em;
                color: dodgerblue;
                font-family: Impact, Haettenschweiler;
            }
            .title{
                vertical-align: middle;
                position: fixed;
                color: dodgerblue;
                font-size: 3.5em;
                text-decoration: underline;
                margin-top: 1%;
                margin-left: 2%;
            }
            .tag_class{
                color: dodgerblue;
                font-size: 1.2em;
            }

            .container-1{
                width: 96%;
                height: 100%;
                top: 1%;
                position: absolute;
                display: inline-block;
                text-align: center;
            }
            .container-1 .icon{
                font-size: 23px;
                position: absolute;
                margin-top: 5px;
                margin-left: 220px;
                z-index: 1;
                color: dodgerblue;
               text-align: right;
            }
           .bottom{
                position:fixed;
                bottom: 0;
                width: 100%;
                height: 30px;
                background-color: dodgerblue;
            }

        </style>
    </head>
    <body>
    <div class="top"></div>
    <div class="title">
        <span style="font-family: Impact, Haettenschweiler">Search Result</span>
    </div>
    <div class="container-1">
        <div class="box1">
            <span class="tag_class">Object</span>
            <div class="img_box">

            </div>
        </div>
        <div class="vertical_bar">

        </div>
        <div class="box2">
            <span class="tag_class">Text</span>
            <div class="img_box">

            </div>
        </div>
    </div>
    <div class="bottom"></div>
    </body>
</html>